{
  "name": "oxc-elements",
  "version": "0.1.0",
  "private": true,
  "main": "./lib/index.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "author": {
    "name": "Alluma"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-brands-svg-icons": "^5.13.1",
    "@fortawesome/free-regular-svg-icons": "^5.13.1",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@fortawesome/pro-light-svg-icons": "^5.13.1",
    "@fortawesome/pro-regular-svg-icons": "^5.13.1",
    "@fortawesome/pro-solid-svg-icons": "^5.13.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "5.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@svgr/webpack": "5.4.0",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.2.1",
    "@testing-library/user-event": "^11.2.1",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "8.1.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.2",
    "camelcase": "^6.0.0",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "chokidar": "^3.4.0",
    "cpx": "1.5.0",
    "cross-env": "7.0.2",
    "css-loader": "3.5.3",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.2.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "5.1.3",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "file-loader": "6.0.0",
    "fs-extra": "^9.0.1",
    "highlight.js": "9.10.0",
    "html-webpack-plugin": "4.3.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.0.1",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "26.0.1",
    "jest-watch-typeahead": "0.6.0",
    "mini-css-extract-plugin": "0.9.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.6.4",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "9.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.2",
    "react-addons-test-utils": "^15.6.2",
    "react-app-polyfill": "^1.0.6",
    "react-dev-utils": "^10.2.1",
    "react-docgen": "^5.3.0",
    "react-test-renderer": "^16.13.1",
    "resolve": "1.17.0",
    "resolve-url-loader": "3.1.1",
    "rimraf": "^3.0.2",
    "sass-loader": "8.0.2",
    "semver": "7.3.2",
    "style-loader": "1.2.1",
    "terser-webpack-plugin": "3.0.3",
    "ts-pnp": "1.2.0",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-dev-server": "3.11.0",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "5.1.3"
  },
  "scripts": {
    "prestart": "npm run gen:docs",
    "start": "npm-run-all --parallel start:docs gen:docs-watch",
    "start:docs": "node scripts/start.js",
    "gen:docs": "node scripts/generateComponentData.js",
    "gen:docs-watch": "npm run gen:docs -- --watch",
    "build:css": "cpx \"./src/components/**/*.css\" ./lib",
    "prebuild:lib": "rimraf lib",
    "build:copy-files": "node scripts/copyBuildFiles.js",
    "build:lib": "npm-run-all --parallel build:commonjs build:css build:copy-files",
    "build:docs": "node scripts/build.js",
    "build:commonjs": "cross-env NODE_ENV=production babel ./src/components --out-dir ./lib --ignore spec.js",
    "prepublish": "npm run build:lib",
    "test": "node scripts/test.js --env=jsdom",
    "refreshVSToken": "vsts-npm-auth -config .npmrc"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": "commonjs"
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
